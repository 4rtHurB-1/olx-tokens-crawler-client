{
  "name": "crawler",
  "version": "0.0.0",
  "private": true,
  "main": "./bin/www.js",
  "scripts": {
    "start": "node ./bin/www",
    "make-exec": "pkg --output exec/crawler ."
  },
  "bin": "./bin/www.js",
  "pkg": {
    "assets": [
      "node_modules/**/*",
      "/**/*"
    ],
    "targets": [
      "node14-win-x64"
    ]
  },
  "dependencies": {
    "express": "~4.14.0",
    "morgan": "~1.7.0",
    "node-fetch": "~2.6.6",
    "open": "^8.4.0",
    "pug": "2.0.0-beta6",
    "puppeteer-core": "^13.0.0"
  }
}
